<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listviewformyapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="31" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.ListViewForMyApp" >
        <activity
            android:name="com.example.listviewformyapp.signUpScreen"
            android:exported="false" />
        <activity
            android:name="com.example.listviewformyapp.ui.login.LoginScreen"
            android:exported="false"
            android:label="@string/title_activity_login_screen" />
        <activity
            android:name="com.example.listviewformyapp.SplashScreen"
            android:exported="false" />
        <activity android:name="com.example.listviewformyapp.MainActivity" />
        <activity
            android:name="com.example.listviewformyapp.profession_scree"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>